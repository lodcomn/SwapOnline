AiCpB,QAAQ,CAACkB,WAAT,EAAjC,EAAyDf,IAAzD,CAAZ;AACAS,QAAAA,cAAc,GAAGlC,OAAO,CAACsC,YAAR,CAAqBK,OAArB,CAA6BrB,QAAQ,CAACkB,WAAT,EAA7B,EAAqDf,IAArD,CAAjB;AACD;;AAED,0BACE,oBAAC,KAAD;AAAO,QAAA,IAAI,EAAET,IAAb;AAAmB,QAAA,KAAK,EAAEiB,IAAI,CAACW,aAAL,CAAmBpC,MAAM,CAACC,KAA1B,CAA1B;AAA4D,QAAA,OAAO,EAAE,KAAKoC,WAA1E;AAAuF,QAAA,eAAe,EAAE;AAAxG,sBACE;AAAK,QAAA,SAAS,EAAC;AAAf,sBACE,8CACE;AAAK,QAAA,SAAS,EAAC,WAAf;AAA2B,QAAA,GAAG,EAAExC,KAAhC;AAAuC,QAAA,GAAG,EAAC;AAA3C,QADF,CADF,eAKE;AAAK,QAAA,SAAS,EAAC;AAAf,sBACE;AAAK,QAAA,SAAS,EAAC;AAAf,sBACE,+CAAM,yCAAUgB,MAAV,QAAoBC,QAAQ,CAACwB,WAAT,EAApB,MAAN,CADF,EAEG,CAACjB,UAAD,iBACC,oDAAO,oBAAC,gBAAD;AAAkB,QAAA,EAAE,EAAC,gBAArB;AAAsC,QAAA,cAAc,EAAC;AAArD,QAAP,eACE,+BADF,eAEE,oCAASN,SAAT,CAFF,CAHJ,CADF,CALF,eAiBE;AAAO,QAAA,SAAS,EAAC,oBAAjB;AAAsC,QAAA,SAAS,EAAC;AAAhD,sBACE,gDACE,6CACE;AAAI,QAAA,SAAS,EAAC,QAAd;AAAuB,QAAA,OAAO,EAAE;AAAhC,sBACE,oBAAC,gBAAD;AAAkB,QAAA,EAAE,EAAC,WAArB;AAAiC,QAAA,cAAc,EAAC;AAAhD,QADF,CADF,CADF,eAME,6CACE;AAAI,QAAA,OAAO,EAAE;AAAb,sBACE;AAAG,QAAA,IAAI,EAAEwB,MAAM,CAACC,QAAP,CAAgBC,IAAzB;AAA+B,QAAA,MAAM,EAAC,QAAtC;AAA+C,QAAA,SAAS,EAAC;AAAzD,mBACMb,EAAE,CAACc,KAAH,CAAS,CAAT,EAAY,CAAZ,CADN,gBAC0Bd,EAAE,CAACc,KAAH,CAAS,CAAC,CAAV,CAD1B,EADF,CADF,CANF,eAaE,0CAgBIvB,UAAU,GAAG,CAAd,iBACC,6CACE;AAAI,QAAA,SAAS,EAAC;AAAd,sBACE,oBAAC,gBAAD;AAAkB,QAAA,EAAE,EAAC,sBAArB;AAA4C,QAAA,cAAc,EAAC;AAA3D,QADF,CADF,eAIE,6CACE,oCACGA,UADH,OACgBL,QADhB,CADF,CAJF,CAjBJ,CAbF,CADF,CAjBF,CADF,eAgEE;AAAK,QAAA,SAAS,EAAC;AAAf,sBACE,oBAAC,WAAD;AACE,QAAA,SAAS,EAAE,IADb;AAEE,QAAA,QAAQ,EAAC,OAFX;AAGE,QAAA,IAAI,YAAKrB,aAAa,EAAlB,SAAuBkC,SAAvB,CAHN;AAIE,QAAA,KAAK,EAAEd,MAAM,CAAC8B,QAAP,KAAoB,GAApB,GAA0B7B,QAAQ,CAAC6B,QAAT,EAA1B,GAAgD,GAAhD,GAAsDlB,IAAI,CAACW,aAAL,CAAmBpC,MAAM,CAACI,IAA1B,CAAtD,GAAwF,GAAxF,GAA8FW;AAJvG,QADF,CAhEF,CADF;AA0ED;;;;EAlKkC5B,KAAK,CAACyD,S;SAAtBvC,O","sourcesContent":["import React, { Component } from 'react'\r\nimport { Modal } from 'components/modal'\r\nimport { FormattedMessage, defineMessages, injectIntl } from 'react-intl'\r\nimport helpers from \"helpers\";\r\nimport { getFullOrigin } from 'helpers/links'\r\n\r\nimport cssModules from 'react-css-modules'\r\nimport styles from './InfoPay.scss'\r\nimport ShareButton from 'components/controls/ShareButton/ShareButton'\r\nimport okSvg from 'shared/images/ok.svg'\r\nimport actions from 'redux/actions'\r\n\r\n\r\n\r\nimport animateFetching from 'components/loaders/ContentLoader/ElementLoading.scss'\r\n\r\n\r\n\r\nconst labels = defineMessages({\r\n  Title: {\r\n    id: 'InfoPay_1',\r\n    defaultMessage: 'Transaction is completed',\r\n  },\r\n  Text: {\r\n    id: 'InfoPay_2',\r\n    defaultMessage: 'successfully transferred to'\r\n  },\r\n\r\n})\r\n@injectIntl\r\n@cssModules({\r\n  ...styles,\r\n  ...animateFetching,\r\n}, { allowMultiple: true })\r\n\r\nexport default class InfoPay extends React.Component<any, any> {\r\n\r\n  props: any\r\n\r\n  constructor(props) {\r\n    super(props)\r\n\r\n    const {\r\n      data: {\r\n        amount,\r\n        currency,\r\n        toAddress,\r\n        txRaw,\r\n        txId,\r\n        balance,\r\n        oldBalance,\r\n        confirmed,\r\n        isFetching,\r\n        onFetching,\r\n      }\r\n    } = props\r\n\r\n    this.state = {\r\n      isFetching,\r\n      amount: isFetching ? 0 : amount,\r\n      currency,\r\n      toAddress: isFetching ? `Fetching` : toAddress,\r\n      balance: isFetching ? 0 : balance,\r\n      oldBalance: isFetching ? 0 : oldBalance,\r\n      confirmed: isFetching ? false : confirmed,\r\n    }\r\n\r\n    if (isFetching && onFetching instanceof Function) {\r\n      onFetching(this)\r\n    }\r\n  }\r\n\r\n  handleClose = () => {\r\n    const { name, data, onClose } = this.props\r\n\r\n    if (typeof onClose === 'function') {\r\n      onClose()\r\n    }\r\n\r\n    if (typeof data.onClose === 'function') {\r\n      data.onClose()\r\n    }\r\n\r\n    actions.modals.close(name)\r\n  }\r\n\r\n\r\n  render() {\r\n    const {\r\n      intl,\r\n      data: {\r\n        currency,\r\n        txRaw,\r\n        txId,\r\n      },\r\n      name,\r\n    } = this.props\r\n\r\n    const {\r\n      isFetching,\r\n      amount,\r\n      toAddress,\r\n      balance,\r\n      oldBalance,\r\n      confirmed,\r\n    } = this.state\r\n\r\n    let linkBlockChain = '#'\r\n    let linkShare = '#'\r\n    let tx = ''\r\n\r\n    if (txRaw) {\r\n      const txInfo = helpers.transactions.getInfo(currency.toLowerCase(), txRaw)\r\n      tx = txInfo.tx\r\n      linkBlockChain = txInfo.link\r\n    }\r\n\r\n    if (txId) {\r\n      tx = txId\r\n      linkShare = helpers.transactions.getTxRouter(currency.toLowerCase(), txId)\r\n      linkBlockChain = helpers.transactions.getLink(currency.toLowerCase(), txId)\r\n    }\r\n\r\n    return (\r\n      <Modal name={name} title={intl.formatMessage(labels.Title)} onClose={this.handleClose} showCloseButton={true}>\r\n        <div styleName=\"blockCenter\">\r\n          <div>\r\n            <img styleName=\"finishImg\" src={okSvg} alt=\"finish\" />\r\n          </div>\r\n\r\n          <div className=\"p-3\">\r\n            <div styleName=\"shortInfoHolder\">\r\n              <span><strong> {amount}  {currency.toUpperCase()} </strong></span>\r\n              {!isFetching && (\r\n                <span> <FormattedMessage id=\"InfoPay_2_copy\" defaultMessage=\"были успешно переданы\" />\r\n                  <br />\r\n                  <strong>{toAddress}</strong>\r\n                </span>\r\n              )}\r\n            </div>\r\n          </div>\r\n\r\n          <table styleName=\"blockCenter__table\" className=\"table table-borderless\">\r\n            <tbody>\r\n              <tr>\r\n                <td styleName=\"header\" colSpan={2}>\r\n                  <FormattedMessage id=\"InfoPay_3\" defaultMessage=\"Transaction ID\" />\r\n                </td>\r\n              </tr>\r\n              <tr>\r\n                <td colSpan={2}>\r\n                  <a href={window.location.href} target=\"_blank\" styleName=\"txLink\">\r\n                    {`${tx.slice(0, 6)}...${tx.slice(-6)}`}\r\n                  </a>\r\n                </td>\r\n              </tr>\r\n              <>\r\n                {/* <tr>\r\n                    <td styleName=\"header\">\r\n                      <FormattedMessage id=\"InfoPay_4\" defaultMessage=\"Est. time to confirmation\" />\r\n                    </td>\r\n                    <td>\r\n                      {confirmed && (\r\n                        <strong>\r\n                          <FormattedMessage id=\"InfoPay_Confirmed\" defaultMessage=\"Confirmed\" />\r\n                        </strong>\r\n                      )}\r\n                      {!confirmed && (\r\n                        <FormattedMessage id=\"InfoPay_NotConfirmed\" defaultMessage=\"~10 mins\" />\r\n                      )}\r\n                    </td>\r\n                  </tr> */}\r\n                {(oldBalance > 0) && (\r\n                  <tr>\r\n                    <td styleName=\"header\">\r\n                      <FormattedMessage id=\"InfoPay_FinalBalance\" defaultMessage=\"Final balance\" />\r\n                    </td>\r\n                    <td>\r\n                      <strong>\r\n                        {oldBalance} {currency}\r\n                      </strong>\r\n                    </td>\r\n                  </tr>\r\n                )}\r\n              </>\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n        <div styleName=\"blockCenter buttonHolder\">\r\n          <ShareButton\r\n            halfWidth={true}\r\n            minWidth=\"200px\"\r\n            link={`${getFullOrigin()}${linkShare}`}\r\n            title={amount.toString() + ' ' + currency.toString() + ' ' + intl.formatMessage(labels.Text) + ' ' + toAddress} />\r\n        </div>\r\n      </Modal>\r\n    )\r\n  }\r\n}\r\n"]}]}