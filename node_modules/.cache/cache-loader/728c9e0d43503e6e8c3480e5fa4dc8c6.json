K,CAACE,GAAN,GAAYf,KAAZ;AACAa,MAAAA,KAAK,CAACG,QAAN,GAAiB,IAAjB;AACD,K;;;;;;;WAlCD,6BAAoB;AAAA;;AAClB,UAAI,KAAKV,KAAL,CAAWW,SAAf,EAA0B;AACxB,aAAKC,UAAL;AACD;;AACDV,MAAAA,UAAU,CAAC,YAAM;AACf,QAAA,MAAI,CAACD,QAAL,CAAc;AACZJ,UAAAA,OAAO,EAAE;AADG,SAAd,EAEG,YAAM;AACPK,UAAAA,UAAU,CAAC,MAAI,CAACG,KAAN,EAAa,IAAb,CAAV;AACD,SAJD;AAKD,OANS,EAMP,CANO,CAAV;AAOD;;;WAyBD,kBAAS;AAAA,wBACsB,KAAKQ,KAD3B;AAAA,UACChB,OADD,eACCA,OADD;AAAA,UACUC,OADV,eACUA,OADV;AAAA,wBAE+B,KAAKE,KAFpC;AAAA,UAECc,QAFD,eAECA,QAFD;AAAA,UAEWC,IAFX,eAEWA,IAFX;AAAA,UAEiBC,SAFjB,eAEiBA,SAFjB;AAIP,UAAMC,kBAAkB,GAAG3B,EAAE,CAAC,WAAD,EAAc;AACzC,mBAAWO,OAD8B;AAEzC,mBAAWC;AAF8B,OAAd,CAA7B;AAKA,UAAMoB,qBAAqB,GAAG5B,EAAE,CAAC,cAAD,EAAiB;AAC/C,mBAAWO,OADoC;AAE/C,mBAAWC,OAFoC;AAG/C,6BAAqBiB,IAAI,KAAK;AAHiB,OAAjB,CAAhC;AAMA,0BACE;AAAK,QAAA,SAAS,EAAEE;AAAhB,sBACE;AAAK,QAAA,SAAS,EAAEC,qBAAhB;AAAuC,QAAA,OAAO,EAAE,KAAKC;AAArD,sBACE;AAAK,QAAA,SAAS,EAAC,SAAf;AAAyB,QAAA,SAAS,EAAEH;AAApC,SACGF,QADH,CADF,CADF,CADF;AASD;;;;EA3EuC1B,S,4CAElB;AACpBuB,EAAAA,SAAS,EAAE;AADS,C,iDAIK;AACzBN,EAAAA,KAAK,EAAEhB,SAAS,CAAC+B;AADQ,C;SANRzB,Y","sourcesContent":["import React, { Component } from 'react'\r\nimport PropTypes from 'prop-types'\r\nimport cx from 'classnames'\r\nimport actions from 'redux/actions'\r\n\r\nimport cssModules from 'react-css-modules'\r\nimport styles from './Notification.scss'\r\n\r\nimport Sound from 'helpers/Sound/alert.mp4'\r\n\r\n\r\n@cssModules(styles, { allowMultiple: true })\r\nexport default class Notification extends Component<any, any> {\r\n\r\n  static defaultProps = {\r\n    soundPlay: true,\r\n  }\r\n\r\n  static childContextTypes = {\r\n    close: PropTypes.func,\r\n  }\r\n\r\n  state = {\r\n    mounted: false,\r\n    removed: false,\r\n  }\r\n\r\n  componentDidMount() {\r\n    if (this.props.soundPlay) {\r\n      this.soundClick()\r\n    }\r\n    setTimeout(() => {\r\n      this.setState({\r\n        mounted: true,\r\n      }, () => {\r\n        setTimeout(this.close, 8000)\r\n      })\r\n    }, 0)\r\n  }\r\n\r\n  close = () => {\r\n    const { name } = this.props\r\n\r\n    this.setState({\r\n      removed: true,\r\n    }, () => {\r\n      setTimeout(() => {\r\n        actions.notifications.hide(name)\r\n      }, 300)\r\n    })\r\n  }\r\n\r\n  handleClick = () => {\r\n    this.close()\r\n    if (this.props.onClick) this.props.onClick()\r\n  }\r\n\r\n  soundClick = () => {\r\n    let audio = new Audio()\r\n    audio.src = Sound\r\n    audio.autoplay = true\r\n  }\r\n\r\n  render() {\r\n    const { mounted, removed } = this.state\r\n    const { children, type, className } = this.props\r\n\r\n    const containerStyleName = cx('container', {\r\n      'mounted': mounted,\r\n      'removed': removed,\r\n    })\r\n\r\n    const notificationStyleName = cx('notification', {\r\n      'mounted': mounted,\r\n      'removed': removed,\r\n      'ErrorNotification': type === 'ErrorNotification',\r\n    })\r\n\r\n    return (\r\n      <div styleName={containerStyleName}>\r\n        <div styleName={notificationStyleName} onClick={this.handleClick}>\r\n          <div styleName=\"content\" className={className}>\r\n            {children}\r\n          </div>\r\n        </div>\r\n      </div>\r\n    )\r\n  }\r\n}\r\n"]}]}